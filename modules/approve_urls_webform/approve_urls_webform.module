<?php

/*
* Implements hook form alter
*/

function approve_urls_webform_form_alter(&$form, \Drupal\Core\Form\FormStateInterface $form_state, $form_id){
    if($form_id == 'custom_short_url_application'){
        // add JS and CSS to the webform
        $form['#attached']['library'][] = 'approve_urls_webform/approve-urls';
    }
}